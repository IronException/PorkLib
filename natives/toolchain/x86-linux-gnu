export CFLAGS		:=	$(CFLAGS) -m32 -flto=auto -fno-fat-lto-objects
export CXXFLAGS		:=	$(CXXFLAGS) -m32 -flto=auto -fno-fat-lto-objects
export LDFLAGS		:=	$(LDFLAGS) -m32 -flto=auto -fno-fat-lto-objects

export CC  := x86_64-linux-gnu-gcc
export CXX := x86_64-linux-gnu-g++
export LD  := x86_64-linux-gnu-g++

export EXT	:=	so
